generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int      @id @default(autoincrement())
  uuid     String   @unique @default(uuid())
  username String   @unique
  email    String   @unique
  password String
  pokemons Pokemon[]
}

